<%existe = false;%>
<%delegacoes.forEach(function(delegacao){%>
  <% if(delegacao.status == "REPROVADA"){%>
    <%existe = true;%>
    <div class="Portfolio">
      <a href="#!" id='view' onclick="pars(<%=JSON.stringify(delegacao)%>)" data-toggle="modal" data-target="#largeModal">
        <img class="card-img" src="http://www.pngmart.com/files/7/Envelope-PNG-Transparent.png" alt="">
      </a>
      <div class="desc"><%=delegacao.tipoSolicitacaoDelegacao.nome%></div>
    </div>
  <%}%>
<%});%>
<%if(!existe){%>
  <h1>0 delegações confirmadas</h1>
<%}%>
